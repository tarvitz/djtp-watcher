[run]
branch = True
omit = 
    /*tests*.py
    /*apps/core/diggpaginator.py
    /*migrations**.py

[report]
exclude_lines =
  pragma: no cover
  def __repr__
  self.fail\(
  raise AssertionError
  raise NotImplementedError
  raise NotImplemented
  if __name__ == .__main__.:
  def __unicode__

[html]
directory = db/reports/coverage
